{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type":"object",
    "title": "Oliva Bet",
    "properties": {
      "Casino": {
      "type": "array",
          "items": {
              "type": "object",
              "properties": {
                  "idUsuario": {
                      "type": "string", 
                      "pattern": "^[0-9]+$"
                  },
                  "nombreUsuario": {
                      "type": "string", 
                      "pattern": "^[A-Z]{1}[a-zA-Z0-9].*$"
                  },
                  "contrasena": {
                      "type": "string", 
                      "pattern": "^[a-zA-Z0-9]{10,}$"
                  },
                  "correoelectronico": {
                      "type": "string", 
                      "pattern": "^[a-z0-9._%+-]+@[a-z0-9._%+-]{1,}[.][a-z]{2,3}$"
                  },
                  "fecha_Alta":{
                      "type":"string",
                      "format":"date-time"
                  },
                  "IP_Registro":{
                      "type":"string",
                      "pattern":"^([0-9]{1,3}[.]){3}[0-9]{1,3}$"
                  }
              },
              "required": ["idUsuario", "nombreUsuario","contrasena","correoelectronico","fecha_Alta","IP_Registro"]
          }
      }
        
      }
  }
  